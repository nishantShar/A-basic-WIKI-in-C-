
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section JavaScript
{
    <script type="text/javascript" src="@Url.Content("/Scripts/CrossLinkScripts/crossLinkScript.js")"></script>
    
}
<div class="row">
    <div class="col-md-12">
        <table>
            <tr>
                <th>Header</th>
                <th>Page Id</th>
                <th>Version</th>
            </tr>   
            @foreach (var page in ViewBag.PageList)
            {
                var pageid = @page.pageId;
                <tr>
                    <td>@Html.ActionLink(((string)@page.header), "ViewPage", "Home", new { pageNo = pageid, version= page.version },null)</td>
                    <td>@page.pageId</td>
                    <td>@page.version</td>
                </tr>
            }
        </table>
    </div>
</div>

